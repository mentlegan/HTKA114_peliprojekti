text = "After running some experiments, I discovered the logic behind the vines and their colors! 

[outline_size=4][outline_color=red][color=white]Red[/color][/outline_color][/outline_size] flowers affect [outline_size=4][outline_color=red][color=white]red[/color][/outline_color][/outline_size] and [outline_size=4][outline_color=purple][color=white]purple[/color][/outline_color][/outline_size] flowers 
[outline_size=4][outline_color=purple][color=white]Purple[/color][/outline_color][/outline_size] flowers affect [outline_size=4][outline_color=darkgreen][color=white]all flowers[/color][/outline_color][/outline_size]
[outline_size=4][outline_color=blue][color=white]Blue[/color][/outline_color][/outline_size] flowers affect [outline_size=4][outline_color=blue][color=white]blue[/color][/outline_color][/outline_size] and [outline_size=4][outline_color=purple][color=white]purple[/color][/outline_color][/outline_size] flowers "
autowrap_mode = 2

