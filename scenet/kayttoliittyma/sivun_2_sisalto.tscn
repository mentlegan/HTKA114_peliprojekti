offset_right = 346.0
offset_bottom = 431.0
theme_override_colors/default_color = Color(0.180392, 0.12549, 0.0823529, 1)
bbcode_enabled = true
text = "The vines here seem to have [outline_size=4][outline_color=black][color=white]two sides[/color][/outline_color][/outline_size]. On one side they look as if they were dead. As I approached the vines, the [outline_size=4][outline_color=black][color=white]healthy side[/color][/outline_color][/outline_size] reacted to the [outline_size=4][outline_color=orange][color=white]pollen[/color][/outline_color][/outline_size] I am carrying with me. I wonder if the color of the flowers on those vines has any connection to how they behave when exposed to the pollen? "
autowrap_mode = 2

